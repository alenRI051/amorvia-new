  "acts": [
    {
      "id": "act1",
      "title": "Act 1 — First Conversations",
      "nodes": [
        {
          "id": "start",
          "type": "goto",
          "text": "",
          "to": "a1_start"
        },
        {
          "id": "a1_start",
          "type": "line",
          "text": "You've been on three dates with Mia/Mark. It's going well. Tonight they ask: 'Where do you see this going?'",
          "to": "a1_define_expectations"
        },
        {
          "id": "a1_define_expectations",
          "type": "choice",
          "text": "How do you set expectations?",
          "choices": [
            {
              "id": "a1_c1",
              "label": "Be clear: co‑parenting comes first; no child introductions for now. [Co-parent Trust +8, Co-parent Tension −4]",
              "to": "a1_share_policy",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 8
                },
                {
                  "meter": "tension",
                  "delta": -4
                }
              ]
            },
            {
              "id": "a1_c2",
              "label": "Keep it vague and avoid the topic. [Co-parent Trust −6, Co-parent Tension +3]",
              "to": "a1_followup_vague",
              "effects": [
                {
                  "meter": "trust",
                  "delta": -6
                },
                {
                  "meter": "tension",
                  "delta": 3
                }
              ]
            }
          ]
        },
        {
          "id": "a1_followup_vague",
          "type": "line",
          "text": "Your date senses hesitation. That may resurface later.",
          "to": "a1_share_policy"
        },
        {
          "id": "a1_share_policy",
          "type": "choice",
          "text": "Do you inform your ex that you're seeing someone?",
          "choices": [
            {
              "id": "a1_c3",
              "label": "Yes — brief, factual message focused on co‑parent logistics. [Co-parent Trust +9, Co-parent Tension −6]",
              "to": "a1_ex_reacts",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 9
                },
                {
                  "meter": "tension",
                  "delta": -6
                }
              ]
            },
            {
              "id": "a1_c4",
              "label": "No — keep it private for now. [Co-parent Trust −3, Co-parent Tension +6]",
              "to": "a1_ex_reacts",
              "effects": [
                {
                  "meter": "trust",
                  "delta": -3
                },
                {
                  "meter": "tension",
                  "delta": 6
                }
              ]
            }
          ]
        },
        {
          "id": "a1_ex_reacts",
          "type": "choice",
          "text": "Your ex asks if you're seeing someone.",
          "choices": [
            {
              "id": "a1_c5",
              "label": "Model respect: 'Yes, it's early.' [Co-parent Trust +10, Co-parent Tension −7]",
              "to": "a1_end",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 10
                },
                {
                  "meter": "tension",
                  "delta": -7
                }
              ]
            },
            {
              "id": "a1_c6",
              "label": "Get defensive: 'It's none of your business.' [Co-parent Trust −12, Co-parent Tension +14]",
              "to": "a1_end",
              "effects": [
                {
                  "meter": "trust",
                  "delta": -12
                },
                {
                  "meter": "tension",
                  "delta": 14
                }
              ]
            }
          ]
        },
        {
          "id": "a1_end",
          "type": "goto",
          "text": "",
          "to": "act2"
        }
      ]
    },
    {
      "id": "act2",
      "title": "Act 2 — Boundaries & Introductions",
      "nodes": [
        {
          "id": "a2_start",
          "type": "line",
          "text": "Two weeks later, your date asks about meeting your child.",
          "to": "a2_intro_policy"
        },
        {
          "id": "a2_intro_policy",
          "type": "choice",
          "text": "What is your introduction policy?",
          "choices": [
            {
              "id": "a2_c1",
              "label": "Wait 3–6 months of stability. [Co-parent Trust +7, Child Stress −6]",
              "to": "a2_ex_coord",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 7
                },
                {
                  "meter": "childStress",
                  "delta": -6
                }
              ]
            },
            {
              "id": "a2_c2",
              "label": "Early casual meet this weekend. [Co-parent Tension +4, Child Stress +10]",
              "to": "a2_ex_coord",
              "effects": [
                {
                  "meter": "tension",
                  "delta": 4
                },
                {
                  "meter": "childStress",
                  "delta": 10
                }
              ]
            }
          ]
        },
        {
          "id": "a2_ex_coord",
          "type": "choice",
          "text": "Coordinate with your ex?",
          "choices": [
            {
              "id": "a2_c3",
              "label": "Yes — propose simple guideline. [Co-parent Trust +8, Co-parent Tension −6]",
              "to": "a2_child_talk",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 8
                },
                {
                  "meter": "tension",
                  "delta": -6
                }
              ]
            },
            {
              "id": "a2_c4",
              "label": "No — do it silently. [Co-parent Trust −7, Co-parent Tension +9]",
              "to": "a2_child_talk",
              "effects": [
                {
                  "meter": "trust",
                  "delta": -7
                },
                {
                  "meter": "tension",
                  "delta": 9
                }
              ]
            }
          ]
        },
        {
          "id": "a2_child_talk",
          "type": "choice",
          "text": "How do you talk to your child?",
          "choices": [
            {
              "id": "a2_c5",
              "label": "Simple script: 'This is my friend.' [Co-parent Trust +4, Child Stress −7]",
              "to": "a2_end",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 4
                },
                {
                  "meter": "childStress",
                  "delta": -7
                }
              ]
            },
            {
              "id": "a2_c6",
              "label": "No prep — hope for the best. [Co-parent Trust −3, Child Stress +9]",
              "to": "a2_end",
              "effects": [
                {
                  "meter": "trust",
                  "delta": -3
                },
                {
                  "meter": "childStress",
                  "delta": 9
                }
              ]
            }
          ]
        },
        {
          "id": "a2_end",
          "type": "goto",
          "text": "",
          "to": "act3"
        }
      ]
    },
    {
      "id": "act3",
      "title": "Act 3 — Friction & Repair",
      "nodes": [
        {
          "id": "a3_start",
          "type": "line",
          "text": "Your ex hears from the child about a 'new friend' and confronts you.",
          "to": "a3_conflict"
        },
        {
          "id": "a3_conflict",
          "type": "choice",
          "text": "How do you respond?",
          "choices": [
            {
              "id": "a3_c1",
              "label": "Validate feelings + restate child‑first plan. [Co-parent Trust +11, Co-parent Tension −9, Child Stress −5]",
              "to": "a3_partner_ping",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 11
                },
                {
                  "meter": "tension",
                  "delta": -9
                },
                {
                  "meter": "childStress",
                  "delta": -5
                }
              ]
            },
            {
              "id": "a3_c2",
              "label": "Blame and escalate. [Co-parent Trust −14, Co-parent Tension +16, Child Stress +9]",
              "to": "a3_partner_ping",
              "effects": [
                {
                  "meter": "trust",
                  "delta": -14
                },
                {
                  "meter": "tension",
                  "delta": 16
                },
                {
                  "meter": "childStress",
                  "delta": 9
                }
              ]
            }
          ]
        },
        {
          "id": "a3_partner_ping",
          "type": "line",
          "text": "Late-night message from ex: 'Enjoy your new life.'",
          "to": "a3_ping_choice"
        },
        {
          "id": "a3_ping_choice",
          "type": "choice",
          "text": "How do you reply?",
          "choices": [
            {
              "id": "a3_c3",
              "label": "Calm, logistics-only reply. [Co-parent Trust +6, Co-parent Tension −5]",
              "to": "a3_child_regress",
              "effects": [
                {
                  "meter": "trust",
                  "delta": 6
                },
                {
                  "meter": "tension",
                  "delta": -5
                }
              ]
            },
            {
              "id": "a3_c4",
              "label": "Engage and argue back. [Co-parent Trust −8, Co-parent Tension +10]",
              "to": "a3_child_regress",
              "effects": [
                {
                  "meter": "trust",
                  "delta": -8
                },
                {
                  "meter": "tension",
                  "delta": 10
                }
              ]
            }
          ]
        },
        {
          "id": "a3_child_regress",
          "type": "line",
          "text": "Bedtime wobble: your child is restless and clingy.",
          "to": "a3_bedtime_choice"
        },
        {
          "id": "a3_bedtime_choice",
          "type": "choice",
          "text": "What do you do?",
          "choices": [
            {
              "id": "a3_c5",
              "label": "Reassure and keep routine. [Child Stress −8]",
              "to": "a3_outcomes",
              "effects": [
                {
                  "meter": "childStress",
                  "delta": -8
                }
              ]
            },
            {
              "id": "a3_c6",
              "label": "Ignore it, hope it passes. [Child Stress +9]",
              "to": "a3_outcomes",
              "effects": [
                {
                  "meter": "childStress",
                  "delta": 9
                }
              ]
            }
          ]
        },
        {
          "id": "a3_outcomes",
          "type": "end",
          "text": "You can recalibrate pacing and communication based on what happened."
        }
      ]
    }
  ]
}