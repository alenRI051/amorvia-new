    {
      "id": "act3",
      "title": "Act 3 — Boundary Breach / Unexpected Overlap",
      "start": "a3_line1",
      "steps": {
        "a3_line1": {
          "id": "a3_line1",
          "type": "line",
          "text": "At pickup, your new partner is at your place when your ex arrives. The child sees them say hello.",
          "to": "a3_choice1",
          "choices": [
            { "id": "C", "label": "Continue", "effects": [], "next": "a3_choice1" }
          ]
        },
        "a3_choice1": {
          "id": "a3_choice1",
          "type": "choice",
          "text": "Your ex looks surprised. How do you respond in the moment?",
          "choices": [
            {
              "id": "a3_c1",
              "label": "Own the miss: brief apology + redirect to child’s comfort.",
              "to": "a3_line2a",
              "effects": [
                { "meter": "trust", "delta": 3 },
                { "meter": "tension", "delta": -2 },
                { "meter": "childStress", "delta": 0 }
              ],
              "next": "a3_line2a"
            },
            {
              "id": "a3_c2",
              "label": "Downplay it; treat as casual coincidence.",
              "to": "a3_line2b",
              "effects": [
                { "meter": "trust", "delta": -3 },
                { "meter": "tension", "delta": 4 },
                { "meter": "childStress", "delta": 1 }
              ],
              "next": "a3_line2b"
            },
            {
              "id": "a3_c3",
              "label": "Ask partner to step out for privacy.",
              "to": "a3_line2c",
              "effects": [
                { "meter": "trust", "delta": 2 },
                { "meter": "tension", "delta": -1 }
              ],
              "next": "a3_line2c"
            }
          ]
        },
        "a3_line2a": {
          "id": "a3_line2a",
          "type": "line",
          "text": "You acknowledge the situation and focus on the child’s comfort.",
          "to": "a3_choice2",
          "choices": [
            { "id": "C", "label": "Continue", "effects": [], "next": "a3_choice2" }
          ]
        },
        "a3_line2b": {
          "id": "a3_line2b",
          "type": "line",
          "text": "You minimize it; tension rises.",
          "to": "a3_choice2",
          "choices": [
            { "id": "C", "label": "Continue", "effects": [], "next": "a3_choice2" }
          ]
        },
        "a3_line2c": {
          "id": "a3_line2c",
          "type": "line",
          "text": "Your partner steps out; you keep the exchange short and calm.",
          "to": "a3_choice2",
          "choices": [
            { "id": "C", "label": "Continue", "effects": [], "next": "a3_choice2" }
          ]
        },
        "a3_choice2": {
          "id": "a3_choice2",
          "type": "choice",
          "text": "Set the follow-up boundary:",
          "choices": [
            {
              "id": "a3_c2a",
              "label": "Propose a 2–3 minute plan to sync on expectations later (away from child).",
              "to": "a3_end",
              "effects": [
                { "meter": "trust", "delta": 4 },
                { "meter": "tension", "delta": -3 }
              ],
              "next": "a3_end"
            },
            {
              "id": "a3_c2b",
              "label": "Say it’s no big deal; no follow-up needed.",
              "to": "a3_end",
              "effects": [
                { "meter": "trust", "delta": -4 },
                { "meter": "tension", "delta": 6 },
                { "meter": "childStress", "delta": 2 }
              ],
              "next": "a3_end"
            },
            {
              "id": "a3_c2c",
              "label": "Text later acknowledging their perspective and clarifying pacing.",
              "to": "a3_end",
              "effects": [
                { "meter": "trust", "delta": 3 },
                { "meter": "tension", "delta": -2 }
              ],
              "next": "a3_end"
            }
          ]
        },
        "a3_end": {
          "id": "a3_end",
          "type": "end",
          "text": "End of Act 3.",
          "choices": [
            { "id": "E", "label": "Finish", "effects": [], "next": null }
          ]
        }
