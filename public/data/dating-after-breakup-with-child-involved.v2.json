        },
        "a2_choice1": {
          "id": "a2_choice1",
          "type": "choice",
          "text": "How do you open the topic?",
          "choices": [
            {
              "id": "a2_c1",
              "label": "Neutral, child-first heads-up.",
              "to": "a2_line2a",
              "effects": [
                { "meter": "trust", "delta": 5 },
                { "meter": "tension", "delta": -3 }
              ],
              "next": "a2_line2a"
            },
            {
              "id": "a2_c2",
              "label": "Share lots of details.",
              "to": "a2_line2b",
              "effects": [
                { "meter": "trust", "delta": -4 },
                { "meter": "tension", "delta": 6 },
                { "meter": "childStress", "delta": 2 }
              ],
              "next": "a2_line2b"
            },
            {
              "id": "a2_c3",
              "label": "Avoid the topic.",
              "to": "a2_line2c",
              "effects": [
                { "meter": "trust", "delta": -2 }
              ],
              "next": "a2_line2c"
            }
          ]
        },
        "a2_line2a": {
          "id": "a2_line2a",
          "type": "line",
          "text": "You signal pacing and no intros yet.",
          "to": "a2_choice2",
          "choices": [
            { "id": "C", "label": "Continue", "effects": [], "next": "a2_choice2" }
          ]
        },
        "a2_line2b": {
          "id": "a2_line2b",
          "type": "line",
          "text": "They tense up at the amount of info.",
          "to": "a2_choice2",
          "choices": [
            { "id": "C", "label": "Continue", "effects": [], "next": "a2_choice2" }
          ]
        },
        "a2_line2c": {
          "id": "a2_line2c",
          "type": "line",
          "text": "You kick the can again.",
          "to": "a2_choice2",
          "choices": [
            { "id": "C", "label": "Continue", "effects": [], "next": "a2_choice2" }
          ]
        },
        "a2_choice2": {
          "id": "a2_choice2",
          "type": "choice",
          "text": "Set a boundary about pacing and info:",
          "choices": [
            {
              "id": "a2_c2a",
              "label": "90-day no intros + only schedule-relevant info.",
              "to": "a2_end",
              "effects": [
                { "meter": "trust", "delta": 6 },
                { "meter": "tension", "delta": -4 }
              ],
              "next": "a2_end"
            },
            {
              "id": "a2_c2b",
              "label": "Intros soon; it’s going great.",
              "to": "a2_end",
              "effects": [
                { "meter": "trust", "delta": -6 },
                { "meter": "tension", "delta": 8 },
                { "meter": "childStress", "delta": 3 }
              ],
              "next": "a2_end"
            },
            {
              "id": "a2_c2c",
              "label": "Ask what helps them feel safe.",
